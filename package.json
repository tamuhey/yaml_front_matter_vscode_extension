{
    "name": "yaml-sign",
    "displayName": "Yaml Sign",
    "description": "",
    "version": "0.0.1",
    "publisher": "tamuhey",
    "engines": {
        "vscode": "^1.28.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:yaml_sign.sign"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "yaml_sign.sign",
                "title": "Yaml Sign"
            }
        ],
        "configuration": [
            {
                "type": "object",
                "title": "config ",
                "properties": {
                    "yaml_sign.author": {
                        "type":"string",
                        "default":null
                    }
                }
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.21",
        "tslint": "^5.8.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    }
}
